# http server config
httpServer:
  host: "0.0.0.0"

# web3 config
web3:
  provider: "https://<rpc>"
  EIP1559: true

# entry point
entryPoint:
  address: "<0xaddress>"

# bundler config
bundler:
  # seconds [2,60]
  interval: 15
  helper: "<0xaddress>"
  # max gas 0.5 ETH
  maxGas: 500000000000000000
  beneficiary: "<0xaddress>"
  privateKeys:
    ["<0xprivate key>"]
