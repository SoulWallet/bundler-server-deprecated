# http server config
httpServer:
  host: "0.0.0.0"

# web3 config
web3:
  provider: "<web3 provider url [http/https]>"

# entry point
entryPoint:
  address: "<entry point address>"

# paymaster config
paymaster:
  stakePaymasterAddress: "0x<address>"
  stakeSignatureKey: "0x<privateKey>"
  freePaymasterAddress: "0x<address>"
  freeSignatureKey: "0x<privateKey>"

# bundler config
bundler:
  interval: 5 # seconds [2,60]
  privateKeys: ["0x<privateKey1>", "0x<privateKey2>", "0x<privateKey3>"]
